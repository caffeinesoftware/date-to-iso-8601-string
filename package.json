{
  "name": "date-to-iso-8601-string",
  "version": "1.0.0",
  "description": "Presents a JavaScript `Date` as a string in [ISO 8601 format](https://www.loc.gov/standards/datetime/iso-tc154-wg5_n0038_iso_wd_8601-1_2016-02-16.pdf), with support for the Extended (`YYYY-MM-DD`) and Basic (`YYYYMMDD`) variants",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "homepage": "https://github.com/caffeinesoftware/date-to-iso-8601-string",
  "files": [
    "dist"
  ],
  "keywords": ["date", "iso", "iso8601", "iso-8601"],
  "scripts": {
    "test": "jest",
    "build": "npx tsc -p tsconfig.json && tsc -p tsconfig-commonjs.json",
    "prepublish": "npm run build",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "author": "Caffeine Software Ltd <hello@caffeinesoftware.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
